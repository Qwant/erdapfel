{
  "name": "erdapfel",
  "version": "1.0.0",
  "description": "Qwant Maps frontend",
  "main": "public/index.html",
  "homepage": "http://qwant.github.io/erdapfel",
  "scripts": {
    "bench": "node ./bench/performances.js",
    "unit-test": "jest --config=./tests/unit.js",
    "integration-test-mobile": "TEST_DEVICE=mobile jest --config=./tests/integration.js",
    "integration-test-desktop": "TEST_DEVICE=desktop jest --config=./tests/integration.js",
    "integration-test": "npm run integration-test-mobile -- --runInBand && npm run integration-test-desktop -- --runInBand",
    "test": "npm run unit-test && npm run integration-test",
    "start": "node bin/index.js",
    "build": "webpack --config build/webpack.config.js",
    "build:clean": "rimraf public/build/",
    "i18n": "./i18n.sh",
    "i18n-pull": "tx pull --all --force",
    "watch": "webpack --config build/webpack.config.js -w --mode=development",
    "msgmerge": "node local_modules/merge-po/index",
    "create-compilation-hash": "node build/createCompilationHash.js",
    "prebuild": "npm run build:clean && npm run create-compilation-hash",
    "prewatch": "npm run build:clean && npm run create-compilation-hash",
    "prepare": "if [ \"$IGNORE_PREPARE\" != true ]; then node build/before_build.js; fi",
    "lint": "eslint src bin build tests --ext '.js,.jsx,.ts,.tsx'",
    "lint-fix": "eslint --fix src bin build tests --ext '.js,.jsx,.ts,.tsx'",
    "prettier:check": "prettier --check '{src,bin,build,tests}/**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write '{src,bin,build,tests}/**/*.{js,jsx,ts,tsx}'",
    "analyze": "webpack --config build/webpack.config.js --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json public/build/javascript",
    "ts:coverage": "typescript-coverage-report",
    "ts:count": "cloc src --fullpath --include-lang TypeScript,JavaScript --force-lang='Javascript',jsx --not-match-d \"(node_modules|coverage-ts|tmp|build)\" --by-percent cm",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy-storybook": "npm run build-storybook -s public",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Qwant/erdapfel.git"
  },
  "author": "Qwant",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.9.0",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.9.0",
    "@mapbox/mapbox-gl-rtl-text": "^0.2.3",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^16.14.23",
    "@types/react-dom": "^16.9.14",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^9.7.5",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.0.4",
    "classnames": "^2.3.1",
    "cloc": "^2.9.0",
    "css-loader": "^5.2.7",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "2.0.0",
    "fs-extra": "^7.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-extended": "^0.11.2",
    "json-loader": "0.5.7",
    "lint-staged": "^10.5.4",
    "mapbox-gl": "^1.13.0",
    "mapbox-gl-js-mock": "github:Qwant/mapbox-gl-js-mock#72d1f4b",
    "postcss-import": "12.0.0",
    "postcss-loader": "2.0.9",
    "prettier": "^2.2.1",
    "puppeteer": "^2.0.0",
    "puppeteer-har": "^1.1.2",
    "qwant-maps-fonts": "^1.0.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.39.0",
    "sass-loader": "^10.2.0",
    "selfsigned": "^1.10.8",
    "style-loader": "^2.0.0",
    "supertest": "^3.3.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^8.3.0",
    "typescript-coverage-report": "^0.6.4",
    "url-loader": "^4.1.1",
    "url-search-params-polyfill": "^5.1.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.12",
    "yaml-jest": "^1.2.0",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "@qwant/map-style-builder": "github:Qwant/map-style-builder#81eebb0",
    "@qwant/qwant-basic-gl-style": "^1.4.0",
    "@qwant/qwant-ponents": "^0.2.1",
    "@turf/along": "^6.0.1",
    "@turf/bbox": "^6.0.1",
    "@turf/bearing": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/invariant": "^6.1.2",
    "@turf/length": "^6.0.2",
    "@turf/line-slice-along": "^5.1.5",
    "@turf/meta": "^6.0.2",
    "alt-route-labeller": "^0.3.1",
    "axios": "^0.21.2",
    "bunyan": "^1.8.12",
    "color": "^3.1.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.21.1",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.3",
    "express-validator": "^6.9.2",
    "finalhandler": "^1.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2",
    "nock": "^10.0.0",
    "node-yaml": "^4.0.1",
    "prom-client": "^11.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-remove-scroll": "^2.5.4",
    "typescript": "^4.5.5",
    "use-delayed-state": "^1.2.0",
    "zustand": "^3.7.1"
  },
  "workspaces": [
    "./local_modules/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "typeCoverage": {
    "ignoreFiles": [
      "*/**/*.jsx",
      "*/**/*.js"
    ],
    "atLeast": 80
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "16.14.2",
    "npm": "8.5.0"
  }
}
