{
  "name": "erdapfel",
  "version": "1.0.0",
  "description": "Qwant Maps frontend",
  "main": "public/index.html",
  "scripts": {
    "bench": "node ./bench/performances.js",
    "unit-test": "jest --config=./tests/unit.js",
    "integration-test-mobile": "TEST_DEVICE=mobile jest --config=./tests/integration.js",
    "integration-test-desktop": "TEST_DEVICE=desktop jest --config=./tests/integration.js",
    "integration-test": "npm run integration-test-mobile && npm run integration-test-desktop",
    "test": "npm run unit-test && npm run integration-test",
    "start": "node bin/index.js",
    "build": "webpack --config build/webpack.config.js",
    "i18n": "./i18n.sh",
    "i18n-pull": "tx pull --all --force",
    "watch": "webpack --config build/webpack.config.js -w --mode=development",
    "msgmerge": "node local_modules/merge-po/index",
    "prepare": "if [ \"$NODE_ENV\" != production ]; then node build/before_build.js; fi",
    "lint": "eslint src bin build tests --ext '.js,.jsx'",
    "lint-fix": "eslint --fix src bin build tests --ext '.js,.jsx'",
    "analyze": "webpack --config build/webpack.config.js --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json public/build/javascript"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Qwant/erdapfel.git"
  },
  "author": "Qwant Research",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/plugin-transform-parameters": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-transform-template-literals": "^7.8.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.0",
    "@babel/runtime": "^7.9.0",
    "@mapbox/mapbox-gl-rtl-text": "^0.2.3",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "2.0.0",
    "fs-extra": "^7.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.2",
    "json-loader": "0.5.7",
    "mapbox-gl": "^1.13.0",
    "mapbox-gl-js-mock": "github:Qwant/mapbox-gl-js-mock#72d1f4b",
    "node-sass": "^4.14.1",
    "postcss-import": "12.0.0",
    "postcss-loader": "2.0.9",
    "puppeteer": "^2.0.0",
    "puppeteer-har": "^1.1.2",
    "qwant-maps-fonts": "^1.0.0",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "6.0.6",
    "selfsigned": "^1.10.8",
    "supertest": "^3.3.0",
    "url-search-params-polyfill": "^5.1.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.12",
    "yaml-jest": "^1.0.5",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.0",
    "@qwant/map-style-builder": "github:Qwant/map-style-builder#9808391",
    "@qwant/qwant-basic-gl-style": "^1.1.0",
    "@turf/along": "^6.0.1",
    "@turf/bbox": "^6.0.1",
    "@turf/bearing": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/invariant": "^6.1.2",
    "@turf/length": "^6.0.2",
