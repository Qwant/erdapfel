    "@qwant/nconf-getter": "file:local_modules/nconf_getter",
    "@qwant/po-js-loader": "file:local_modules/po-js-loader",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.2.0",
    "babel-loader": "^8.0.4",
    "detect-browser": "^4.7.0",
