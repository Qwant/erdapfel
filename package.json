    "compression": "^1.7.3",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.3",
    "express-validator": "^6.9.2",
    "finalhandler": "^1.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2",
    "nock": "^10.0.0",
    "node-yaml": "^3.2.0",
    "prom-client": "^11.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "request": "^2.88.0"
  },
  "workspaces": [
    "./local_modules/*"
  ]
}
