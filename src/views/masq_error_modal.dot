<div class="modal__masq_error">
  <div class="modal__masq_error__header">
    <div class="icon-masq modal__masq_error__logo"></div>
    <i class="icon-x modal__masq_error__close" {{= click(this.close, this) }}></i>
  </div>

  <div class="modal__masq_error__body">
    <h2 class="modal__masq_error__title">{{= this.title }}</h2>
    <h2 class="modal__masq_error__subtitle">{{= this.subtitle }}</h2>


    <div class="modal__masq_error__buttons">
      <button class="modal__masq_error__button" {{= click(this.close, this) }}>{{= _('Close') }}</button>
    </div>
  </div>
</div>
